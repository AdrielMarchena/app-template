{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/Game/src/**",
                "${workspaceFolder}/Game/vendor/**",
                "${workspaceFolder}/Game/vendor/entt/single_include/"
            ],
            "forcedInclude": [
                "${default}"
            ],
            "defines": [],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "gnu17",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "${default}",
            "configurationProvider": "ms-vscode.makefile-tools",
            "browse": {
                "path": [
                    "${workspaceFolder}/",
                    "${workspaceFolder}/Game/src/",
                    "${workspaceFolder}/Game/vendor/",
                    "${workspaceFolder}/Game/vendor/entt/single_include/"
                ]
            }
        }
    ],
    "version": 4
}